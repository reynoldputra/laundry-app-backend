generator client {
  provider = "prisma-client-js"
  // binaryTargets = ["native", "linux-musl-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

model user {
  id     String      @id @default(uuid())
  name   String      @db.VarChar(99)
  email  String      @unique @db.VarChar(99)
  status user_status
}

enum user_status {
  active
  inactive
}
