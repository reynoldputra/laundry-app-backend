{
  "name": "vite-starter",
  "private": true,
  "version": "0.0.0",
  "main": "./src/main.js",
  "prisma": {
    "schema": "src/database/prisma/schema.prisma"
  },
  "scripts": {
    "dev": "nodemon src/main.js -w src",
    "start": "NODE_ENV=production node src/main.js",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:host": "DB_HOST=localhost npx prisma generate",
    "db:push": "npx prisma db push",
    "db:push:host": "DB_HOST=localhost npx prisma db push",
    "migrate-dev": "npx prisma migrate dev",
    "migrate-dev:host": "DB_HOST=localhost npx prisma migrate dev",
    "build": "vite build"
  },
  "imports": {
    "#src/*": "./src/*"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "vite": "^4.0.4"
  },
  "dependencies": {
    "@js-joda/core": "^5.6.1",
    "@prisma/client": "^5.5.2",
    "bcrypt": "^5.1.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "http-errors": "^2.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^5.5.2",
    "vite-express": "*",
    "zod": "^3.22.4",
    "zod-joda": "^1.1.3"
  }
}
